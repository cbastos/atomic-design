{
  "name": "app.spa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "setup": "node ./npm-scripts/install/run.js && npm rebuild node-sass",
    "start": "npm run start:dev",
    "start:dev": "npm run build:dev",
    "start:pro": "npm run build:pro && node ./dist/app/app.js",
    "build:services": "node npm-scripts/build/services/generate.js",
    "build:barrel": "node npm-scripts/build/buildBarrel.js",
    "build:html": "node npm-scripts/build/buildIndexHtml.js",
    "build:dll:dev": "webpack --config ./npm-scripts/build/webpack.dll.dev.js",
    "prebuild:dev": "npm run setup && npm run build:services && npm run build:barrel && npm run build:html -- development && npm run build:dll:dev",
    "build:dev": "webpack-dev-server --host 0.0.0.0 --config npm-scripts/build/webpack.config.dev.js --hot --progress --colors --content-base dist/app",
    "build:dll:pro": "webpack --config ./npm-scripts/build/webpack.dll.pro.js",
    "prebuild:pro": "npm run setup && npm run build:services && npm run build:barrel && npm run build:html -- production && npm run build:dll:pro",
    "build:pro": "webpack --config npm-scripts/build/webpack.config.pro.js",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "lint": "eslint modules/**/*.js",
    "docs": "esdoc -c ./npm-scripts/docs/esdoc.json"
  },
  "private": true,
  "dependencies": {
    "@progress/kendo-drawing": "^1.5.7",
    "@progress/kendo-react-dateinputs": "^1.1.0",
    "@progress/kendo-react-dropdowns": "^1.1.0",
    "@progress/kendo-react-grid": "^1.1.0",
    "@progress/kendo-react-inputs": "^1.1.0",
    "@progress/kendo-react-intl": "^1.1.0",
    "@progress/kendo-react-pdf": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "4.0.0-alpha.6",
    "elegant-icons-sass": "^0.7.0",
    "express": "^4.16.2",
    "moment": "^2.20.1",
    "oidc-client": "1.3.0",
    "react": "16.2.0",
    "react-beautiful-dnd": "^4.0.1",
    "react-dom": "16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0",
    "sass": "^0.5.0",
    "superagent": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-jest": "^22.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.25.0",
    "cssnano": "^3.10.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "esdoc": "^1.0.4",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-publish-html-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "glob": "^7.1.2",
    "jest": "^22.2.2",
    "jsdom": "^11.3.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "swagger-js-codegen": "^1.12.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/npm-scripts/test/shim.js",
      "<rootDir>/npm-scripts/test/jest.setup.js"
    ],
    "moduleNameMapper": {
      "connector": "<rootDir>/modules/one-rate/components/connector.jsx",
      "components": "<rootDir>/modules/one-rate/components/barrel.generated.js",
      "domain": "<rootDir>/modules/one-rate/domain",
      "core": "<rootDir>/modules/one-rate/shared"
    },
    "coverageDirectory": "./dist/test/coverage",
    "globals": {
      "window": true
    }
  }
}
